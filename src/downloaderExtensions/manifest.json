{"name": "__MSG_extName__",
 "version": "1.0.16",
 "manifest_version": 2,
 "description": "__MSG_extDesc__",
 "icons": {"128": "icon128.png"},
 "browser_action": {
   "default_icon": {
     "19": "icon19.png",
     "38": "icon38.png"},
   "default_title": "__MSG_extName__"},
 "background": {"persistent": true, "scripts": ["background.js"]},
 "default_locale": "en",
 "optional_permissions": ["management"],
 "permissions": [
    "downloads",
    "downloads.open",
    "downloads.shelf",
    "cookies",
    "notifications",
    "tabs",
    "contextMenus",
    "https://*.com/*",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "nativeMessaging",
    "storage"
     ]}
